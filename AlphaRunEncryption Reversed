function stepOne(str){
    var alpha = "abcdefghijklmnopqrstuvwxyz"
    var array = []
  	for(var i = 0 ; i < str.length-1;i++){
    	 var currentLetter = alpha.indexOf(str[i])
         var nextLetter = alpha.indexOf(str[i+1])
         if (nextLetter > currentLetter){
         for(var j = currentLetter+1; j < nextLetter; j++){
         	array.push(alpha[j])
          }
        } else {
     	 for(var k = currentLetter-1; k > nextLetter; k--){
         	array.push(alpha[k])
         }		
       }
    }
   return array.join("")
}


