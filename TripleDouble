function TripleDouble(num1,num2) { 
	var number1 = num1.toString().split("")
    var number2 = num2.toString().split("")
	for(var i = 0 ; i < number2.length;i++){
    	if(number2[i] == number2[i+1]){
  			for(var k = 0 ; k < number1.length;k++){
            	if(number1[k] == number2[i] && number1[k]==number1[k+1]&& number1[k+2]){
                	return 1
                }
            }      
        }
      return 0
    }
  
