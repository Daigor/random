function StringScramble(str1,str2) { 
	for(var i = 0 ; i < str2.length;i++){
      	var count = 0
        var count2 = 0 
    	for(var j = 0 ; j < str2.length; j++){
        	if(str2[i] == str2[j]){
            	count += 1
            }
        }
    	for(var k = 0 ; k < str1.length;k++){
        	if(str2[i] == str1[k]){
            	count2 +=1
            }
        }
      if(count != count2){return false}
    }
  return true
}
